{
  "presets": ["react-native-stage-0"],
  "plugins": [
    ["module-alias", [
      { "src": "./src", "expose": "src" },
      { "src": "./src/core", "expose": "core" },
      { "src": "./src/pages", "expose": "pages" },
      { "src": "./src/components", "expose": "components" },
      { "src": "./src/utils", "expose": "utils" },
      { "src": "./src/reducers", "expose": "reducers" },
      { "src": "./src/core/redux/middlewares", "expose": "middlewares" },
      { "src": "./config.js", "expose": "config" },
    ]]
  ]
}
